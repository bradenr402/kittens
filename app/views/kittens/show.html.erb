<%= link_to 'Home', root_path %> |
<%= link_to 'Edit', edit_kitten_path %> |
<%= link_to 'Delete', kitten_path(@kitten), data: { turbo_method: :delete, turbo_confirm: "Are you sure you want to delete this kitten?" } %>

<h1>
  <%= @kitten.name %>
</h1>

<ul>
  <li>
    Age: <%= @kitten.age %>
  </li>
  <li>
    Cuteness: <%= @kitten.cuteness %>
  </li>
  <li>
    Softness: <%= @kitten.softness %>
  </li>
</ul>
